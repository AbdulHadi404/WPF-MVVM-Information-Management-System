                                <ListViewItem x:Name="Item_AddReceipt" Selected="Item_AddReceipt_Selected">Add Receipt</ListViewItem>
                                <ListViewItem x:Name="Item_RecipientList" Selected="Item_RecipientList_Selected">Recipient List</ListViewItem>
